{
  "version": 2,
  "routes": [
    {
      "src": "/api/generate-video",
      "dest": "/api/generate-video.js"
    },
    {
      "src": "/api/video-status",
      "dest": "/api/video-status.js"
    },
    {
      "src": "/api/videos",
      "dest": "/api/videos.js"
    },
    {
      "src": "/api/upload-video",
      "dest": "/api/upload-video.js"
    },
    {
      "src": "/api/artists",
      "dest": "/api/artists.js"
    },
    {
      "src": "/api/video-folders",
      "dest": "/api/video-folders.js"
    },
    {
      "src": "/api/delete-video",
      "dest": "/api/delete-video.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/generate-video.js": {
      "maxDuration": 10
    },
    "api/video-status.js": {
      "maxDuration": 5
    },
    "api/videos.js": {
      "maxDuration": 5
    },
    "api/upload-video.js": {
      "maxDuration": 60
    },
    "api/artists.js": {
      "maxDuration": 5
    },
    "api/video-folders.js": {
      "maxDuration": 10
    },
    "api/delete-video.js": {
      "maxDuration": 10
    }
  }
}

{
  "version": 2,
  "routes": [
    {
      "src": "/api/generate-video",
      "dest": "/api/generate-video.js"
    },
    {
      "src": "/api/video-status",
      "dest": "/api/video-status.js"
    },
    {
      "src": "/api/videos",
      "dest": "/api/videos.js"
    },
    {
      "src": "/api/upload-video",
      "dest": "/api/upload-video.js"
    },
    {
      "src": "/api/artists",
      "dest": "/api/artists.js"
    },
    {
      "src": "/api/video-folders",
      "dest": "/api/video-folders.js"
    },
    {
      "src": "/api/delete-video",
      "dest": "/api/delete-video.js"
    },
    {
      "src": "/api/archive-upload",
      "dest": "/api/archive-upload.js"
    },
    {
      "src": "/api/archive-status",
      "dest": "/api/archive-upload.js"
    },
    {
      "src": "/api/archive-manifest",
      "dest": "/api/archive-upload.js"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/manage-artists",
      "dest": "/api/manage-artists.js"
    },
    {
      "src": "/api/deploy-website",
      "dest": "/api/deploy-website.js"
    },
    {
      "src": "/api/redirect",
      "dest": "/api/redirect.js"
    },
    {
      "src": "/api/update-website",
      "dest": "/api/deploy-website.js"
    },
    {
      "src": "/website/(.*)",
      "dest": "/website/$1"
    },
    {
      "src": "/",
      "dest": "/api/redirect.js",
      "headers": {
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/generate-video.js": {
      "maxDuration": 10
    },
    "api/video-status.js": {
      "maxDuration": 5
    },
    "api/videos.js": {
      "maxDuration": 5
    },
    "api/upload-video.js": {
      "maxDuration": 60
    },
    "api/artists.js": {
      "maxDuration": 5
    },
    "api/video-folders.js": {
      "maxDuration": 10
    },
    "api/delete-video.js": {
      "maxDuration": 10
    },
    "api/archive-upload.js": {
      "maxDuration": 120
    },
    "api/upload.js": {
      "maxDuration": 300
    },
    "api/manage-artists.js": {
      "maxDuration": 60
    },
    "api/deploy-website.js": {
      "maxDuration": 300
    },
    "api/redirect.js": {
      "maxDuration": 10
    }
  }
}
